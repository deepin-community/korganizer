#: alarmdialog.cpp:1000
#, kde-format
msgctxt "@info"
msgid "Could not start KOrganizer so editing is not possible."
msgstr "Nie można uruchomić KOrganizer, więc edytowanie nie jest możliwe."

#: alarmdialog.cpp:1008
#, kde-format
msgctxt "@info"
msgid "An internal KOrganizer error occurred attempting to modify \"%1\""
msgstr "Wystąpił błąd wewnętrzny KOrganizera przy próbie zmiany \"%1\""

